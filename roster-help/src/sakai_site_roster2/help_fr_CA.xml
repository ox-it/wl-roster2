<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
   <bean id="roster2Overview" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aruh2</value></property>
      <property name="name"><value>Roster Overview</value></property>
      <property name="location"><value>/sakai_site_roster2/aruh.html</value></property>
      <property name="defaultForTool"><value>sakai.site.roster2</value></property>
   </bean>

   <bean id="roster2View" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arna2</value></property>
      <property name="name"><value>Viewing your roster</value></property>
      <property name="location"><value>/sakai_site_roster2/arna.html</value></property>
   </bean>

   <bean id="roster2Print" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>atzt2</value></property>
      <property name="name"><value>Printing your roster</value></property>
      <property name="location"><value>/sakai_site_roster2/atzt.html</value></property>
   </bean>

   <bean id="roster2Export" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>atfv2</value></property>
      <property name="name"><value>Export your roster</value></property>
      <property name="location"><value>/sakai_site_roster2/atfv.html</value></property>
   </bean>

    <bean id="org.sakaiproject.api.app.help.TableOfContents" 
          class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
      <property name="name"><value>root</value></property>
      <property name="categories">
         <list>
            <bean id="assignmentCategory" class="org.sakaiproject.component.app.help.model.CategoryBean">
               <property name="name"><value>Roster2</value></property>
               <property name="resources">
                  <list>
                     <ref bean="roster2Overview"/>
                     <ref bean="roster2View"/>
                     <ref bean="roster2Print"/>
                     <ref bean="roster2Export"/>
                  </list>
               </property>
            </bean>
           </list>
         </property>
       </bean>
</beans>
